{
  "name": "mdr-simulator2",
  "version": "1.0.0",
  "description": "MDR Simulator version 2",
  "private": "true",
  "main": "index.js",
  "scripts": {
    "server:watch": "rm -rf dist/*; cp -r server/views dist; nodemon --watch server --ext js,ejs --exec npm run server:build\\; npm run server:run",
    "server:build": "rm -rf dist/*; cp -r server/views dist; babel -d dist server",
    "server:run": "cd dist; node server",
    "client:watch": "rm -rf public/*; webpack  --watch --progress",
    "client:build": "rm -rf public/*; webpack --progress",
    "clean": "npm cache clean --force"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.10.1",
