{
  "name": "mdr-simulator2",
  "version": "1.0.0",
  "description": "MDR Simulator version 2",
  "private": "true",
  "main": "index.js",
  "scripts": {
    "server:watch": "nodemon --watch server --exec npm run server:build\\; npm run server:run",
    "server:build": "babel -d dist server",
    "server:run": "cd dist; node server",
    "client:watch": "brunch watch --server",
    "client:build": "brunch build --production",
    "client:browserify": "browserify app/main.js -o public/main.js -t [ babelify --presets [ @babel/preset-env @babel/preset-react @babel/preset-flow] --plugins [ transform-class-properties ] ] -g uglifyify --no-sourcemap",
    "clean": "npm cache clean --force"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "auto-reload-brunch": "^2.7.1",
    "babel-brunch": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "brunch": "^2.10.17",
    "flow-brunch": "^0.2.4",
    "uglify-js-brunch": "^2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CCMi-FIT/mdr-simulator2-js.git"
  },
  "author": "Robert Pergl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CCMi-FIT/mdr-simulator2-js/issues"
  },
  "homepage": "https://github.com/CCMi-FIT/mdr-simulator2-js#readme",
  "dependencies": {
    "ajv": "^6.5.4",
    "bootstrap3": "^3.3.5",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^4.1.0",
    "express": "^4.16.4",
    "jquery": "^3.3.1",
    "morgan": "^1.9.1",
    "proper-lockfile": "^3.0.2",
    "ramda": "^0.25.0",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-bootstrap-typeahead": "^3.2.4",
    "react-confirm-bootstrap": "^5.3.0",
    "react-dom": "^16.5.2",
    "react-draggable": "^3.0.5",
    "vis": "^4.21.0"
  }
}
