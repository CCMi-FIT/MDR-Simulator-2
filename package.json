{
  "name": "mdr-simulator2",
  "version": "1.0.0",
  "description": "MDR Simulator version 2",
  "private": "true",
  "main": "index.js",
  "scripts": {
    "server:watch": "rm -rf dist/*; cp -r server/views dist; nodemon --watch server --exec npm run server:build\\; npm run server:run",
    "server:build": "rm -rf dist/*; cp -r server/views dist; babel -d dist server",
    "server:run": "cd dist; node server",
    "client:watch": "rm -rf public/*; brunch watch --server",
    "client:build": "rm -rf public/*; brunch build --production",
    "clean": "npm cache clean --force"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-react-jsx-source": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.10.1",
    "auto-reload-brunch": "^2.7.1",
    "babel-brunch": "^7.0.1",
    "babel-loader": "^8.0.6",
    "brunch": "^2.10.17",
    "flow-brunch": "^0.2.4",
    "uglify-js-brunch": "^2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CCMi-FIT/mdr-simulator2-js.git"
  },
  "author": "Robert Pergl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CCMi-FIT/mdr-simulator2-js/issues"
  },
  "homepage": "https://github.com/CCMi-FIT/mdr-simulator2-js#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@tinymce/tinymce-react": "^3.3.0",
    "ajv": "^6.10.2",
    "bootstrap": "^4.3.1",
    "cartesian-product": "^2.1.2",
    "chalk": "^2.4.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
